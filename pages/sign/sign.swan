            <view class="zhufuicon_wrap">
                <image class="zhifu_icon" src="../../images/wx.png"></image>
            </view>
            <view class="zhifu_wenzi">微信支付</view>
            <view class="wx_m_wrap">
                <view class="fukuan">请用微信扫码付款</view>
                <view class="fukuan_border">微信扫码，快捷付款</view>
            </view>
        </view>
