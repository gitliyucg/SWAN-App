<view class="logo_img"></view>
<view class="wrap clearBoth">
    <view class="sign_list float_right">
        <view class="sign_list_title">活动报名环节内容介绍</view>
        <view class="list clearBoth" s-for="item in list">
            <view class="list_top" data-id="{{item['id']}}" data-type="{{item['activity_type']}}" bindtap="toList"></view>
            <view class="float_left">{{item['title']}}</view>
            <view class="float_right">></view>
        </view>
    </view>
</view>
<view class="zhifu_wrap">
    <view class="zhifu_title">支付方式</view>
    <view class="zhifu clearBoth">
